      <doc xml:space="preserve">Contains only private data.</doc>
      <implements name="Gio.Initable"/>
      <virtual-method name="refresh">
        <return-value transfer-ownership="none">
          <type name="none" c:type="void"/>
        </return-value>
        <parameters>
