      <return-value transfer-ownership="none">
        <type name="GLib.Quark" c:type="GQuark"/>
      </return-value>
    </function>
    <function name="book_utils_get_recipient_certificates_sync"
              c:identifier="e_book_utils_get_recipient_certificates_sync"
              version="3.30"
              throws="1">
      <doc xml:space="preserve">Synchronously searches for @recipients S/MIME or PGP certificates either
in provided @only_clients #EBookClient, or, when %NULL, in each found
address book configured for auto-completion.

This function can be used within camel_session_get_recipient_certificates_sync()
implementation.</doc>
      <return-value transfer-ownership="none">
        <doc xml:space="preserve">%TRUE when no fatal error occurred, %FALSE otherwise.</doc>
        <type name="gboolean" c:type="gboolean"/>
      </return-value>
      <parameters>
        <parameter name="registry" transfer-ownership="none">
          <doc xml:space="preserve">an #ESourceRegistry</doc>
          <type name="EDataServer.SourceRegistry" c:type="ESourceRegistry*"/>
        </parameter>
        <parameter name="only_clients"
                   transfer-ownership="none"
                   nullable="1"
                   allow-none="1">
          <doc xml:space="preserve">optional #GSList of
   the #EBookClient objects to search for the certificates in, or %NULL</doc>
          <type name="GLib.SList" c:type="const GSList*">
            <type name="BookClient"/>
          </type>
        </parameter>
        <parameter name="flags" transfer-ownership="none">
          <doc xml:space="preserve">bit-or of #CamelRecipientCertificateFlags</doc>
          <type name="guint32" c:type="guint32"/>
        </parameter>
        <parameter name="recipients" transfer-ownership="none">
          <doc xml:space="preserve">a #GPtrArray of recipients' email addresses</doc>
          <array name="GLib.PtrArray" c:type="const GPtrArray*">
            <type name="utf8"/>
          </array>
        </parameter>
        <parameter name="out_certificates"
                   direction="out"
                   caller-allocates="0"
                   transfer-ownership="full">
          <doc xml:space="preserve">a #GSList of gathered certificates
   encoded in base64</doc>
          <type name="GLib.SList" c:type="GSList**">
            <type name="utf8"/>
          </type>
        </parameter>
        <parameter name="cancellable"
                   transfer-ownership="none"
                   nullable="1"
                   allow-none="1">
          <doc xml:space="preserve">optional #GCancellable object, or %NULL</doc>
          <type name="Gio.Cancellable" c:type="GCancellable*"/>
        </parameter>
      </parameters>
    </function>
  </namespace>
