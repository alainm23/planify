        <parameter name="state"
                   direction="out"
                   caller-allocates="0"
                   transfer-ownership="full">
          <doc xml:space="preserve">ydecode state</doc>
          <type name="gint" c:type="gint*"/>
        </parameter>
        <parameter name="pcrc"
                   direction="out"
                   caller-allocates="0"
                   transfer-ownership="full">
          <doc xml:space="preserve">part crc state</doc>
          <type name="guint32" c:type="guint32*"/>
