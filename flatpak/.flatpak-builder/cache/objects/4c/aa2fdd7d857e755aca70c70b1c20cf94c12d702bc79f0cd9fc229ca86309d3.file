      <return-value transfer-ownership="full">
        <doc xml:space="preserve">the best-effort SPDX license string</doc>
        <type name="utf8" c:type="gchar*"/>
      </return-value>
      <parameters>
        <parameter name="license" transfer-ownership="none">
          <doc xml:space="preserve">a not-quite SPDX license string, e.g. "GPLv3+"</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
      </parameters>
    </function>
